<h1>Dashboard</h1>
<h3>Welcome  {{name}}</h3>
<p>here your stories</p>

{{#if stories}}
<table class="striped">
    <thead>
        <tr>
            <th>Story</th>
            <th>Date</th>
            <th>status</th>
            </tr>
            </thead>
            <tbody>
                {{#each stories}}
                <tr>
                     <td><a href="/stories/{{_id}}">{{title}}</a></td>
            <td>{{formatDate createdAt 'MMMM Do YYYY, h:mm:ss a'}}</td>
            <td><span class="dash-status">{{status}}</span></td>
            <td>
                </tr>

                {{/each}}
            </tbody>
</table>

{{else}}
<p>no stories</p>
{{/if}}